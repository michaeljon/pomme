{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "WozParser",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ConsoleTools/WozParse/bin/Debug/net10.0/WozParse.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ConsoleTools/WozParse",
      "console": "internalConsole",
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Assembler",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ConsoleTools/Asm6502/bin/Debug/net10.0/Asm6502.dll",
      "args": [],
      "cwd": "${workspaceFolder}/ConsoleTools/Asm6502",
      "console": "internalConsole",
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Disassembler",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ConsoleTools/Dasm6502/bin/Debug/net10.0/Dasm6502.dll",
      "args": [
        "--input",
        "${workspaceFolder}/ConsoleTools/Emu6502/roms/apple2e.rom",
        "--output",
        "-"
      ],
      "cwd": "${workspaceFolder}/ConsoleTools/Dasm6502",
      "console": "internalConsole",
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Simulator",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ConsoleTools/Sim6502/bin/Debug/net10.0/Sim6502.dll",
      "args": ["--input", "tests/simple.S", "--origin", "32768"],
      "cwd": "${workspaceFolder}/ConsoleTools/Sim6502",
      "console": "internalConsole",
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Emulator (single step)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ConsoleTools/Emu6502/bin/Debug/net10.0/Emu6502.dll",
      "args": ["--speed", "0", "--single-step"],
      "cwd": "${workspaceFolder}/ConsoleTools/Emu6502",
      "console": "internalConsole",
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Emulator (trace)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ConsoleTools/Emu6502/bin/Debug/net10.0/Emu6502.dll",
      "args": ["--speed", "0", "--trace"],
      "cwd": "${workspaceFolder}/ConsoleTools/Emu6502",
      "console": "externalTerminal",
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Emulator (console)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/ConsoleTools/Emu6502/bin/Debug/net10.0/Emu6502.dll",
      "args": ["--speed", "0"],
      "cwd": "${workspaceFolder}/ConsoleTools/Emu6502",
      "console": "externalTerminal",
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Emulator (graphical)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/InnoWerks.Emulators.AppleIIe/bin/Debug/net10.0/InnoWerks.Emulators.AppleIIe.dll",
      "cwd": "${workspaceFolder}/InnoWerks.Emulators.AppleIIe",
      "args": ["--disk2", "disks/blank.dsk"],
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Emulator (a2audit)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/InnoWerks.Emulators.AppleIIe/bin/Debug/net10.0/InnoWerks.Emulators.AppleIIe.dll",
      "cwd": "${workspaceFolder}/InnoWerks.Emulators.AppleIIe",
      "args": ["--disk1", "disks/audit.dsk"],
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": "Emulator (a2diag)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/InnoWerks.Emulators.AppleIIe/bin/Debug/net10.0/InnoWerks.Emulators.AppleIIe.dll",
      "cwd": "${workspaceFolder}/InnoWerks.Emulators.AppleIIe",
      "args": ["--disk1", "disks/A2eDiagnostics_v2.1.dsk"],
      "stopAtEntry": false,
      "requireExactSource": false
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "requireExactSource": false
    },
    {
      "name": "C#: Launch Startup Project",
      "type": "dotnet",
      "request": "launch"
    }
  ]
}
